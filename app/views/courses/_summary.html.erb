    <div class='second content'>
      <div class="actions">
        <%= link_to "âœ˜", "/admin/unassign/lesson/#{@lesson.id}/from/course/#{course.id}",
        method: :delete, data: { confirm: 'Are you sure?' }, :class => "delete" if admin? %>

      </div>
    </div>
  </div>
  <div class='expandable'>
    <%= course.description %>

    <%= render partial: 'tags/list', locals: {resource_type: "course", tags: course.tags} %>
  </div>
</div>
